{
  "name": "@yolominds/seacows-amm-subgraph",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "yarn codegen && graph build",
    "prepare:local:hardhat": "mustache config/dev/hardhat.json subgraph.template.yaml > subgraph.yaml",
    "create:local:hardhat": "graph create --node http://localhost:8020/ seacows/seacows-amm-subgraph",
    "remove:local:hardhat": "graph remove --node http://localhost:8020/ seacows/seacows-amm-subgraph",
    "deploy:local:hardhat": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 seacows/seacows-amm-subgraph",
    "prepare:local:goerli": "mustache config/dev/goerli.json subgraph.template.yaml > subgraph.yaml",
    "create:local:goerli": "graph create --node http://localhost:8020/ seacows/seacows-amm-subgraph",
    "remove:local:goerli": "graph remove --node http://localhost:8020/ seacows/seacows-amm-subgraph",
    "deploy:local:goerli": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 seacows/seacows-amm-subgraph",
    "prepare:dev:goerli": "mustache config/dev/goerli.json subgraph.template.yaml > subgraph.yaml",
    "create:dev:goerli": "graph create --node https://subgraph-goerli-dev.seacows.io/deploy seacows/seacows-amm-subgraph",
    "deploy:dev:goerli": "graph deploy --node https://subgraph-goerli-dev.seacows.io/deploy --ipfs https://ipfs-dev.seacows.io seacows/seacows-amm-subgraph --version-label 1.0.0",
    "remove:dev:goerli": "graph remove --node https://subgraph-goerli-dev.seacows.io/deploy seacows/seacows-amm-subgraph"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.49.0",
    "@graphprotocol/graph-ts": "^0.30.0",
    "@yolominds/seacows-sdk": "^1.20.6"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.2",
    "fs": "^0.0.1-security",
    "matchstick-as": "^0.5.0",
    "mustache": "^4.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
